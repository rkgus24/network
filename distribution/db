CREATE TABLE TBL_INOUT_202002 (
	T_NO CHAR(8) NOT NULL,
	P_CODE CHAR(3),
	T_TYPE CHAR(1),
	T_CNT NUMBER(4),
	T_DATE DATE,
	C_CODE CHAR(3),
	PRIMARY KEY(T_NO)
);

