CREATE TABLE TBL_COMPANY_202002 (
	C_CODE CHAR(3) NOT NULL,
	C_NAME VARCHAR2(20),
	C_TEL1 CHAR(3),
	C_TEL2 CHAR(4),
	C_TEL3 CHAR(4),
	PRIMARY KEY(C_CODE)
);

