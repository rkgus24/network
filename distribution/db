CREATE TABLE TBL_PRODUCT_202002 (
	P_CODE CHAR(3) NOT NULL,
	P_NAME VARCHAR2(20),
	P_SIZE NUMBER(4),
	P_INCOST NUMBER(7),
	P_OUTCOST NUMBER(7),
	PRIMARY KEY(P_CODE)
);

